{% extends '::base.html.twig' %}

{% block body -%}
    <h1>FormatoInscripcion list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Pdf</th>
                <th>Imagenheader</th>
                <th>Descripcion</th>
                <th>Precios</th>
                <th>Formato</th>
                <th>Emailenvio</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('backend_formatos_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.pdf }}</td>
                <td>{{ entity.imagenHeader }}</td>
                <td>{{ entity.descripcion }}</td>
                <td>{{ entity.precios }}</td>
                <td>{{ entity.formato }}</td>
                <td>{{ entity.emailEnvio }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('backend_formatos_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('backend_formatos_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('backend_formatos_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
